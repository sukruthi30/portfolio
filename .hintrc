{
  "extends": [
    "development"
  ],
  "hints": {
    "detect-css-reflows/composite": "off",
    "detect-css-reflows/layout": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "min-height: auto"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "disown-opener": "off"
  }
}